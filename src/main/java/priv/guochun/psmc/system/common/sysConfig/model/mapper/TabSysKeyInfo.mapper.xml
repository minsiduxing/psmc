<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.guochun.psmc.system.common.sysConfig.model.mapper.TabSysKeyInfoMapper">

    <resultMap id="BaseResultMap" type="priv.guochun.psmc.system.common.sysConfig.model.TabSysKeyInfo">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="sys_key" jdbcType="VARCHAR" property="sysKey" />
        <result column="sys_value" jdbcType="VARCHAR" property="sysValue" />
        <result column="remark" jdbcType="VARCHAR" property="remark" />
  </resultMap>


  <select id="selectSysKeyListInfo" parameterType="hashmap" resultType="java.util.Map">
    	select * from tab_sys_key_info where 1=1 
    	<if test="sysKey != null and sysKey != ''">
    		and sys_key = #{sysKey,jdbcType=VARCHAR}
    	</if>
    	<if test="sysValue != null and sysValue != ''">
    		and sys_value = #{sysValue,jdbcType=VARCHAR}
    	</if>
        order by id asc
  </select>
</mapper>